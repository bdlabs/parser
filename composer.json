{
    "name": "railt/parser",
    "type": "library",
    "description": "Syntactic parser-analyzer PHP implementation",
    "license": "MIT",
    "keywords": ["parser", "llk", "ll1", "lr", "slr", "language", "php", "ast"],
    "homepage": "http://railt.org",
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/parser"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "ext-spl": "*",
        "ext-pcre": "*",
        "ext-mbstring": "*",
        "railt/io": "1.2.*",
        "railt/lexer": "1.2.*"
    },
    "autoload": {
        "psr-4": {
            "Railt\\Parser\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Tests\\Parser\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
